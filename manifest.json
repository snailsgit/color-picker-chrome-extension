{
    "name":"Color plucker",
    "description":"A color picker extension",
    "version":"1.0",
    "manifest_version":3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "activeTab",
        "scripting"
    ],
    "aboutUs_page":"aboutUs.html",
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "16":"/images/cp_16.png",
        "32":"/images/cp_32.png",
        "64":"/images/cp_64.png"
    }
}